  # You can find the Application ID and Private Key ( will need to be Base64 encoded) in the dashboard: https://dashboard.vonage.com/applications
  VONAGE_APP_ID=
  VONAGE_PRIVATE_KEY64=

  # The URL of the running application without "https://" and trailing "/" (ex. abc123.com). This must be a publically available URL on the internet.
  APP_DOMAIN=

  # Replace with your own subscription key, service region (e.g., "westus"),
  # and recognition language. Can be found in the dashboard: https://portal.azure.com/
  # View full list of languages here: https://learn.microsoft.com/en-us/azure/ai-services/speech-service/language-support?tabs=speech-translation
  AZURE_SUBSCRIPTION_KEY=
  AZURE_SERVICE_REGION=
  DEFAULT_TARGET_LANGUAGE=
